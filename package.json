{
  "name": "just-check",
  "version": "0.0.0",
  "description": "Simple check for installed packages with versions correspond to those specified in the package.json",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "npm run test:1 && npm run test:2",
    "test:1": "node index.js && echo 'OK' || echo 'FAIL'",
    "test:2": "node index.js --install='npm install --legacy-peer-deps' && echo 'OK' || echo 'FAIL'"
  },
  "dependencies": {
    "semver": "^7.5.0"
  },
  "peerDependencies": {
    "semver": "^7.5.0"
  },
  "files": [
    "index.js",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dschewchenko/ssnappy.git"
  },
  "keywords": [
    "dependencies",
    "check",
    "version",
    "install",
    "npm",
    "package"
  ],
  "author": "dschewchenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dschewchenko/ssnappy/issues"
  },
  "homepage": "https://github.com/dschewchenko/ssnappy#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "husky": "^8.0.3",
    "semantic-release": "^21.0.2"
  }
}
