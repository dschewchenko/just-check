{
  "name": "just-check",
  "version": "1.1.0",
  "description": "Simple check for installed packages with versions correspond to those specified in the package.json",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "npm run test:1 && npm run test:2",
    "test:1": "node index.js && echo 'OK' || echo 'FAIL'",
    "test:2": "node index.js --install='npm install --legacy-peer-deps' && echo 'OK' || echo 'FAIL'",
    "semantic-release": "semantic-release",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "semver": "^7.5.0"
  },
  "peerDependencies": {
    "semver": "^7.5.0"
  },
  "files": [
    "index.js",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dschewchenko/just-check.git"
  },
  "keywords": [
    "dependencies",
    "check",
    "version",
    "install",
    "npm",
    "package"
  ],
  "author": "dschewchenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dschewchenko/just-check/issues"
  },
  "homepage": "https://github.com/dschewchenko/just-check#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "semantic-release": "^21.0.2"
  }
}
